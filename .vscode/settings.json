{
    "files.associations": {
        "*.hpp": "cpp",
        "*.cpp": "cpp",
        "iostream": "cpp",
        "string": "cpp",
        "vector": "cpp",
        "unordered_map": "cpp"
    },
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.compilerPath": "/usr/bin/g++",
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.formatOnSave": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true
}